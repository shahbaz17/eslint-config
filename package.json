{
  "name": "@mdsbzalam/eslint-config",
  "version": "0.0.1",
  "description": "An opinionated ESLint config with Airbnb JavaScript, TypeScript, React, and Prettier baked-in.",
  "author": "Mohammad Shahbaz Alam <hey@mdsbzalam.dev>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "javascript.js",
    "typescript.js",
    "rules"
  ],
  "repository": "https://github.com/shahbaz17/eslint-config",
  "bugs": "https://github.com/shahbaz17/eslint-config/issues",
  "keywords": [
    "eslint",
    "airbnb",
    "prettier",
    "typescript",
    "react"
  ],
  "scripts": {
    "lint": "eslint --fix ./**/*.js"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0"
  },
  "devDependencies": {
    "@mdsbzalam/prettier-config": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "prettier": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
